      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "org.calblueprint.RescuingLeftoverCuisine",
      "versionCode": 2,
      "permissions": []
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {}
        }
      ]
    }
  }
