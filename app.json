{
  "expo": {
    "name": "Rescuing Leftover Cuisine",
    "slug": "rlc-mobile",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "primaryColor": "#38A5DB",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/newrlclogo.png",
    "sdkVersion": "37.0.0",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
